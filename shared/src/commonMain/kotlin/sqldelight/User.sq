import kotlin.collections.List;


CREATE TABLE User (
    name TEXT,
    avatar TEXT,
    likes TEXT AS List<Long>
);

removeUser:
DELETE FROM User;

addUser:
INSERT INTO User(name)
VALUES (:name);

getUser:
SELECT * FROM User;

createLikeTable:
INSERT OR REPLACE INTO User(likes)
VALUES (:likes);

like:
UPDATE User
SET likes = :likes;

getLikes:
SELECT likes FROM User;

